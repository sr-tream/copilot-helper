{
    "name": "copilot-helper-pro",
    "publisher": "biebie99",
    "displayName": "Copilot Helper Pro",
    "description": "An extension that provides model support for GitHub Copilot Chat, including ZhipuAI, MiniMax, MoonshotAI, DeepSeek, Alibaba Cloud Bailian, and custom OpenAI/Anthropic compatible models.",
    "version": "0.15.27",
    "license": "MIT",
    "keywords": [
        "ai",
        "chat",
        "copilot",
        "github-copilot",
        "language-model",
        "zhipu",
        "kimi",
        "minimax",
        "moonshot",
        "deepseek",
        "OpenAI Compatible",
        "Anthropic Compatible",
        "ZhipuAI",
        "MoonshotAI",
        "DeepSeek",
        "Alibaba Cloud Bailian",
        "Coding Plan"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/nhatbien/copilot-helper"
    },
    "engines": {
        "vscode": ">=1.104.0",
        "node": ">=20.0.0",
        "npm": ">=9.0.0"
    },
    "extensionDependencies": [
        "github.copilot-chat"
    ],
    "categories": [
        "AI",
        "Chat"
    ],
    "icon": "./logo_ai.png",
    "main": "./dist/extension.js",
    "enabledApiProposals": [
        "chatProvider"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onLanguageModelProvider:chp.zhipu",
        "onLanguageModelProvider:chp.minimax",
        "onLanguageModelProvider:chp.moonshot",
        "onLanguageModelProvider:chp.deepseek",
        "onLanguageModelProvider:chp.codex",
        "onLanguageModelProvider:chp.compatible",
        "onLanguageModelTool:chp_zhipuWebSearch",
        "onLanguageModelTool:chp_minimaxWebSearch",
        "onFileSystem:chp-settings"
    ],
