        "configuration": {
            "title": "Copilot Helper Pro - AI Chat Models",
            "properties": {
                "chp.temperature": {
                    "type": "number",
                    "default": 0.1,
                    "minimum": 0,
                    "maximum": 2,
                    "description": "Controls the randomness of AI model output. Lower values (like 0.1) produce more deterministic output, while higher values (like 1.0) produce more creative output.",
                    "scope": "application"
                },
                "chp.topP": {
                    "type": "number",
                    "default": 1,
                    "minimum": 0,
                    "maximum": 1,
                    "description": "Controls the diversity of AI model output. Using smaller values (like 0.9) reduces output randomness.",
                    "scope": "application"
                },
                "chp.maxTokens": {
                    "type": "number",
                    "default": 8192,
                    "minimum": 32,
                    "maximum": 256000,
                    "description": "Controls the maximum number of tokens in AI model output. Larger values allow longer responses.",
                    "scope": "application"
                },
                "chp.editToolMode": {
                    "type": "string",
                    "enum": [
                        "claude",
                        "gpt-5",
                        "none"
                    ],
                    "enumDescriptions": [
                        "Use Claude-style editing tool (ReplaceString) - efficient and precise single replacement",
                        "Use GPT-5-style editing tool (ApplyPatch) - batch difference application, supports complex refactoring",
                        "Do not use specific optimization tools, direct file editing - lower efficiency, suitable for handling edit exceptions"
                    ],
                    "default": "claude",
                    "description": "Select editing tool mode. Claude uses efficient ReplaceString, GPT-5 uses ApplyPatch for batch editing, None uses basic file editing directly.",
                    "scope": "application"
                },
                "chp.rememberLastModel": {
                    "type": "boolean",
                    "default": true,
                    "description": "When enabled, records and automatically restores the last used extension-provided model. (VSCode 1.106.0 will reset to default optimal model [e.g., Claude Sonnet 4.5])",
                    "scope": "application"
                },
                "chp.zhipu.search.enableMCP": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enable ZhipuAI search MCP SDK mode. Uses standard MCP protocol and official SDK for search functionality, providing more stable connections and better error handling. Non-subscription plans should disable this switch to use standard billing interface (per-request billing). Requires valid ZhipuAI API key.",
                    "scope": "application"
                },
                "chp.zhipu.endpoint": {
                    "type": "string",
                    "enum": [
                        "open.bigmodel.cn",
                        "api.z.ai"
                    ],
                    "default": "open.bigmodel.cn",
                    "description": "ZhipuAI access site. open.bigmodel.cn is domestic site (recommended), api.z.ai is international site.",
                    "scope": "application"
                },
                "chp.minimax.endpoint": {
                    "type": "string",
                    "enum": [
                        "minimaxi.com",
                        "minimax.io"
                    ],
                    "default": "minimaxi.com",
                    "description": "MiniMax Coding Plan access site. minimaxi.com is domestic site (recommended), minimax.io is international site.",
                    "scope": "application"
                },
                "chp.fimCompletion.enabled": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable FIM (Fill In the Middle) suggestion feature.",
                    "scope": "application"
                },
                "chp.fimCompletion.debounceMs": {
                    "type": "number",
                    "default": 500,
                    "minimum": 50,
                    "maximum": 1000,
                    "description": "Debounce delay for FIM completion (milliseconds).",
                    "scope": "application"
                },
                "chp.fimCompletion.timeoutMs": {
                    "type": "number",
                    "default": 5000,
                    "minimum": 1000,
                    "maximum": 30000,
                    "description": "Request timeout for FIM completion (milliseconds).",
                    "scope": "application"
                },
                "chp.fimCompletion.modelConfig": {
                    "type": "object",
                    "default": {
                        "provider": "",
                        "baseUrl": "",
                        "model": ""
                    },
                    "description": "Model configuration for FIM completion.",
                    "scope": "application",
                    "properties": {
                        "provider": {
                            "type": "string",
                            "description": "Model provider identifier"
                        },
                        "baseUrl": {
                            "type": "string",
                            "description": "API base URL",
                            "format": "uri"
                        },
                        "model": {
                            "type": "string",
                            "description": "Model name used for FIM completion"
                        },
                        "maxTokens": {
                            "type": "number",
                            "description": "Maximum length returned by FIM completion",
                            "default": 200,
                            "minimum": 50,
                            "maximum": 16000
                        },
                        "extraBody": {
                            "type": "object",
                            "description": "Additional request body parameters, merged into the request body",
                            "additionalProperties": {
                                "description": "Additional request body parameter values"
                            }
                        }
                    },
                    "required": [
                        "provider",
                        "baseUrl",
                        "model"
                    ]
                },
                "chp.nesCompletion.enabled": {
                    "type": "boolean",
                    "default": false,
                    "description": "Enable NES (Next Edit Suggestions) suggestion feature.",
                    "scope": "application"
                },
                "chp.nesCompletion.debounceMs": {
                    "type": "number",
                    "default": 500,
                    "minimum": 50,
                    "maximum": 1000,
                    "description": "Debounce delay for NES completion (milliseconds).",
                    "scope": "application"
                },
                "chp.nesCompletion.timeoutMs": {
                    "type": "number",
                    "default": 5000,
                    "minimum": 1000,
                    "maximum": 30000,
                    "description": "Request timeout for NES completion (milliseconds).",
                    "scope": "application"
                },
                "chp.nesCompletion.manualOnly": {
                    "type": "boolean",
                    "default": false,
                    "description": "When enabled, NES completion only responds to manual triggers (Alt+/), will not trigger automatically.",
                    "scope": "application"
                },
                "chp.nesCompletion.modelConfig": {
                    "type": "object",
                    "default": {
                        "provider": "",
                        "baseUrl": "",
                        "model": ""
                    },
                    "description": "Model configuration for NES completion.",
                    "scope": "application",
                    "properties": {
                        "provider": {
                            "type": "string",
                            "description": "Model provider identifier"
                        },
                        "baseUrl": {
                            "type": "string",
                            "description": "API base URL",
                            "format": "uri"
                        },
                        "model": {
                            "type": "string",
                            "description": "Model name used for NES completion"
                        },
                        "maxTokens": {
                            "type": "number",
                            "description": "Maximum length returned by NES completion",
                            "default": 200,
                            "minimum": 50,
                            "maximum": 16000
                        },
                        "extraBody": {
                            "type": "object",
                            "description": "Additional request body parameters, will be merged into request body in API requests",
                            "additionalProperties": {
                                "description": "Additional request body parameter value"
                            }
                        }
                    },
                    "required": [
                        "provider",
                        "baseUrl",
                        "model"
                    ]
                },
                "chp.providerOverrides": {
                    "type": "object",
                    "default": {},
                    "description": "Provider configuration override. Allows overriding provider baseUrl and model configuration, supports adding new models or overriding existing model parameters.",
                    "scope": "application"
                },
                "chp.compatibleModels": {
                    "type": "array",
                    "default": [],
                    "description": "Custom model configuration for Compatible Provider. Completely user-configured OpenAI/Anthropic compatible model list.",
                    "scope": "application"
                }
            }
        },
        "jsonValidation": [
            {
                "fileMatch": "settings.json",
                "url": "chp-settings://root/schema.json"
            }
        ],
        "languageModelChatProviders": [
