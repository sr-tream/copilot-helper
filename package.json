        "keybindings": [
            {
                "command": "editor.action.inlineSuggest.trigger",
                "key": "alt+/",
                "when": "editorHasCompletionItemProvider && textInputFocus && !editorReadonly && !suggestWidgetVisible"
            },
            {
                "command": "chp.nesCompletion.toggleManual",
                "key": "shift+alt+/"
            },
            {
                "command": "chp.copilot.attachSelection",
                "key": "ctrl+shift+a",
                "mac": "cmd+shift+a",
                "when": "editorTextFocus && editorHasSelection"
            },
            {
                "command": "chp.copilot.insertHandle",
                "key": "ctrl+shift+h",
                "mac": "cmd+shift+h",
                "when": "editorTextFocus"
            },
            {
                "command": "chp.copilot.insertHandleFullPath",
                "key": "ctrl+alt+shift+h",
                "mac": "cmd+alt+shift+h",
                "when": "editorTextFocus"
            },
            {
                "command": "chp.accounts.quickSwitch",
                "key": "ctrl+shift+q",
                "mac": "cmd+shift+q"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "submenu": "chp.copilot.submenu",
                    "group": "1_copilot",
                    "when": "editorTextFocus"
                }
            ],
            "chp.copilot.submenu": [
                {
                    "command": "chp.copilot.insertHandle",
                    "group": "1_handle@1",
                    "when": "editorTextFocus"
                },
                {
                    "command": "chp.copilot.insertHandleFullPath",
                    "group": "1_handle@2",
                    "when": "editorTextFocus"
                },
                {
                    "command": "chp.copilot.attachSelection",
                    "group": "2_attach@1",
                    "when": "editorTextFocus && editorHasSelection"
                }
            ]
        },
        "submenus": [
            {
                "id": "chp.copilot.submenu",
                "label": "Copilot Helper Pro",
                "icon": "$(copilot)"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run clean && npm run compile",
        "compile": "node esbuild.config.js",
        "compile:dev": "node esbuild.config.js --dev",
        "clean": "rimraf dist",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format": "prettier --write \"src/**/*.{ts,js,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
        "watch": "node esbuild.config.js --watch --dev",
        "package": "vsce package",
        "publish": "vsce publish --allow-proposed-apis chatProvider",
        "pretest": "npm run compile:dev",
        "release": "vsce package && \"${VSCODE_BIN:-/Applications/Visual Studio Code.app/Contents/Resources/app/bin/code}\" --install-extension *.vsix --force",
        "test": "echo \"No tests specified\" && exit 0"
    },
